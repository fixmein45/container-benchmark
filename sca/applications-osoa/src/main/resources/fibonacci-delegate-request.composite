<?xml version="1.0" encoding="ISO-8859-15"?>

<composite xmlns="http://www.osoa.org/xmlns/sca/1.0" name="fibonacci-delegate-request">
  <service name="fibonacci" promote="fibonacci-impl/fibonacci"/>
  
  <component name="fibonacci-impl">
    <implementation.java class="container.java.applications.fibo.lib.FibonacciDelegate"/>
    
    <service name="fibonacci">
      <interface.java interface="container.java.applications.fibo.api.IFibonacci"/>
    </service>
    
    <reference name="delegate" target="fibonacci-impl/fibonacci">
      <interface.java interface="container.java.applications.fibo.api.IFibonacci"/>
    </reference>

  </component>

</composite>
